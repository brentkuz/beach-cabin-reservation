@model BeachCabinReservation.Models.Home.IndexViewModel

@using BeachCabinReservation.Utility;

@{
    ViewData["Title"] = "Home";
}


<div id="calendar"></div>




@section Config{
    @*Injected config*@
    @Html.RenderJsonConfigTags(new Dictionary<string, string>
    {
        {"config",  Model.GetClientConfig()},
        {"urlConfig", Model.GetUrls()}
    })
}
@section scripts{
    <environment include="Development">
        <script src="~/source/home/index.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/dist/home-index.min.js" asp-append-version="true"></script>
    </environment>
}


